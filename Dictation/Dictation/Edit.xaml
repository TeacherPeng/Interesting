<Window x:Class="Dictation.Edit"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Dictation"
        mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=local:WordsModel, IsDesignTimeCreatable=True}"
        Title="编辑" Height="450" Width="800">
    <DockPanel>
        <Menu DockPanel.Dock="Top">
            <MenuItem Header="导入" Click="OnImport_Click" />
        </Menu>
        <DataGrid ItemsSource="{Binding Words}" />
    </DockPanel>
</Window>
